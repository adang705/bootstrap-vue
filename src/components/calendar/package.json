{
  "name": "@bootstrap-vue/caendar",
  "version": "1.0.0",
  "meta": {
    "title": "Calendar",
    "version": "2.5.0",
    "description": "Calendar widget for selecting dates.",
    "components": [
      {
        "component": "BCalendar",
        "version": "2.5.0",
        "props": [
          {
            "prop": "value",
            "description": "Initially selected date value. Accepts either a YYYY-MM-DD string or a date object"
          },
          {
            "prop": "valueAsDate",
            "description": "returns a date object for the v-model instead of a YYYY-MM-DD string"
          }
        ]
      }
    ]
  }
}
